{
  "expo": {
    "name": "FIRE Hub",
    "description": "Visualize your path to retirement!",
    "slug": "firehub",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/flamemintfitted.png",
    "splash": {
      "image": "./assets/flamesplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.holdenmaudlin.firehub",
      "buildNumber": "1",
      "supportsTablet": false,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["reddit"],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.holdenmaudlin.firehub",
      "versionCode": 4,
      "permissions": []
    }
  }
}
